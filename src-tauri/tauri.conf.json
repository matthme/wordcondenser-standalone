{
  "build": {
    "beforeDevCommand": "npm run dev",
    "devPath": "../pouch/ui",
    "distDir": "../pouch/ui",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "Word Condenser",
    "version": "0.0.1"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      }
    },
    "systemTray": {
      "iconPath": "icons/32x32.png"
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "word-condenser-hc-0.2",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "security": {
      "csp": null
    },
    "windows": []
  }
}
